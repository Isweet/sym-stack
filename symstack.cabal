name:                symstack
version:             0.1.0.0
-- synopsis:            
-- description:         
homepage:            https://github.com/Isweet/symstack
license:             GPL-3
license-file:        LICENSE
author:              Ian Sweet
maintainer:          iannsweet@gmail.com
-- copyright:           
category:            Static Analysis
build-type:          Simple
-- extra-source-files:  
cabal-version:       >= 1.10

library
  exposed-modules:     SymStack
  -- other-modules:       
  -- other-extensions:    
  build-depends:       base >= 4.8 && < 4.9, containers >= 0.5 && < 0.6, z3
  hs-source-dirs:      src
  default-language:    Haskell2010

test-suite symstack-test
  type:                exitcode-stdio-1.0
  main-is:             Main.hs
  build-depends:       base >= 4.8 && < 4.9, symstack
  hs-source-dirs:      test
  default-language:    Haskell2010

